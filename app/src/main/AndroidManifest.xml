<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.StudyTime">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudyTime">

        <activity android:name="com.example.StudyTime.AddSessionActivity"
            android:label="@string/add_session_name"/>
        <activity android:name="com.example.StudyTime.RecyclerActivity"
            android:label="@string/session_list_name"/>
        <activity android:name="com.example.StudyTime.CalendarActivity"
            android:label="@string/calendar_name"/>
        <activity android:name="com.example.StudyTime.MainActivity"
            android:label="@string/timer_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>